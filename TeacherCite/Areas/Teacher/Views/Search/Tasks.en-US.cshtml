@model IList<StTask>
@{
    Layout = null;
}

@if (Model.Count != 0)
{
    @foreach (var task in Model)
    {
        <h6><a asp-controller="Task" asp-action="ShowTask" asp-route-StTaskID="@(task.StTaskID)" asp-route-returnUrl="/Teacher/Task/List">@task.Name</a></h6>
    }
}
else
{
   <h4>Nothing was found</h4> <p>Request string was "@ViewBag.String"...</p>
}